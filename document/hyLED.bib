@article{Moreno:06,
author = {Ivan Moreno and Maximino Avenda\~{n}o-Alejo and Rumen I. Tzonchev},
journal = {Appl. Opt.},
keywords = {Illumination; Light-emitting diodes; Illumination design; Image resolution; LED sources; Light sources; Photometry; White light},
number = {10},
pages = {2265--2272},
publisher = {OSA},
title = {Designing light-emitting diode arrays for uniform near-field irradiance},
volume = {45},
month = {Apr},
year = {2006},
url = {http://ao.osa.org/abstract.cfm?URI=ao-45-10-2265},
doi = {10.1364/AO.45.002265},
abstract = {We analyze the first-order design of light sources consisting of multiple light-emitting diodes (LEDs) to uniformly illuminate a near target plane by considering each single LED as an imperfect Lambertian emitter. Simple approximate equations and formulas are derived for the optimum LED-to-LED spacing, i.e., the optimum packaging density, of several array configurations to achieve uniform near-field irradiance.},
}

@article{Barakat:62,
author = {Richard Barakat},
journal = {J. Opt. Soc. Am.},
keywords = {Illuminance; Light properties; Optical devices; Partial coherence; Ptychography; Pupil},
number = {3},
pages = {276--283},
publisher = {OSA},
title = {Application of Apodization to Increase Two-Point Resolution by the Sparrow Criterion. I. Coherent Illumination},
volume = {52},
month = {Mar},
year = {1962},
url = {http://www.osapublishing.org/abstract.cfm?URI=josa-52-3-276},
doi = {10.1364/JOSA.52.000276},
abstract = {A resolution criterion due to Sparrow is studied. Unlike the well-known Rayleigh resolution criterion, the Sparrow resolution criterion is dependent upon the coherence properties of the incident light. The extreme limits of complete coherence and complete incoherence are investigated. An apodization scheme for increasing the coherent Sparrow resolution limit is formulated via the calculus of variations. The determination of the required pupil function (amplitude distribution over the exit pupil) necessitates the solution of an inhomogeneous Fredholm integral equation. Both slit and circular aperture are studied in detail.},
}

@article{Su:12,
author = {Zhouping Su and Donglin Xue and Zhicheng Ji},
journal = {Opt. Express},
keywords = {Light-emitting diodes; Computation methods ; Illumination design ; Nonimaging optics ; Analytical techniques; Arrays; LED lighting; LED sources; Light sources; Photometry},
number = {S6},
pages = {A843--A855},
publisher = {OSA},
title = {Designing LED array for uniform illumination distribution by simulated annealing algorithm},
volume = {20},
month = {Nov},
year = {2012},
url = {http://www.opticsexpress.org/abstract.cfm?URI=oe-20-106-A843},
doi = {10.1364/OE.20.00A843},
abstract = {We propose a numerical optimization method designing LED array for achieving a good uniform illumination distribution on target plane. Simulated annealing algorithm is employed to optimize LED array arrangement. Using the method, we optimized three LED arrays with various luminous intensity profiles. In order to exhibit the design freedom of the method, we use some LEDs with different intensity value in the first and third array, respectively. By optimizing, the three arrays all produced highly uniform illumination distribution with the uniformity of 0.12, 0.23 and 0.13, respectively. It indicates our method can design various luminous intensity distribution LED arrays and design array consisting of LEDs with different intensity value. In addition, the method is simple and can optimize the LED array automatically by computer program. To the best of our knowledge, it is first time to use numerical optimization method to design the optimal LED array arrangement for uniform irradiance.},
}

@article{Pal:15,
author = {Sourav Pal},
journal = {Appl. Opt.},
keywords = {Illumination; Light-emitting diodes; Illumination design ; Illumination design ; Arrays; Genetic algorithms; Illumination design; LED lighting; LED sources; Light sources},
number = {27},
pages = {8221--8227},
publisher = {OSA},
title = {Optimization of LED array for uniform illumination over a target plane by evolutionary programming},
volume = {54},
month = {Sep},
year = {2015},
url = {http://ao.osa.org/abstract.cfm?URI=ao-54-27-8221},
doi = {10.1364/AO.54.008221},
abstract = {An ab initio design of a light emitting diode (LED) array for achieving uniform illumination is presented. An optimization technique based on evolutionary programming has been developed to facilitate the search for an optimal array in the hyperspace formed by a number of LEDs and spacing between them. Numerical results are presented for a regular and irregular array with LEDs having Lambertian and special types of light distribution.},
}

@article{Lei:0,
author = {P. Lei and Q. Wag and H. Zou},
journal = {JEOS:RP},
keywords = {LED; design; uniform illumination; algorithm},
number = {0},
publisher = {EOS},
title = {Designing LED array for uniform illumination based on local search algorithm},
volume = {9},
year = {2014},
url = {http://www.jeos.org/index.php/jeos_rp/article/view/14014},
doi = {10.2971/jeos.2014.14014},
abstract = {We propose a numerical optimization method based on local search algorithm to design an LED array for a highly uniform illumination distribution. In the first place, an initial LED array is randomly generated and the corresponding value of the objective function is calculated. In the second place, the value of the objective function is iteratively improved by applying local changes of the LED array until the objective function value can not be improved. This method can automatically design an array of LEDs with different luminous intensity value and distribution. Computer simulations show that the near-optimal LED array with highly uniform illumination distribution on target plane is obtained by this method.},
}

