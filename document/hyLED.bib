journal = {Opt. Express},
keywords = {Light-emitting diodes; Physical optics; Genetic algorithms; LED lighting; LED sources; Light sources; Visible light communications; White light emitting diodes},
number = {8},
pages = {8659--8669},
publisher = {OSA},
title = {Power allocation for uniform illumination with stochastic LED arrays},
volume = {25},
month = {Apr},
year = {2017},
url = {http://www.opticsexpress.org/abstract.cfm?URI=oe-25-8-8659},
doi = {10.1364/OE.25.008659},
abstract = {In this paper, a simple heuristic power allocation scheme is proposed for a random LED array to obtain uniform irradiance on the projection surface. This is done by considering a binomial point process (BPP) for modeling the LED location and using the quality factor as a performance metric. Numerical results are provided to validate the proposed model and demonstrate its simplicity over existing LED geometries.},
}


